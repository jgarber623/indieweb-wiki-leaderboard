          "properties": {
            "name": [
              "4th compilation of OER textbook"
            ],
            "url": [
              "https://tarduini.com/oer-textbooks/4th-compilation-of-oer-textbook/",
              "https://tarduini.com/oer-textbooks/4th-compilation-of-oer-textbook/"
