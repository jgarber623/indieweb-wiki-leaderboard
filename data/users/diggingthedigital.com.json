        "photo": [
          {
            "value": "http://0.gravatar.com/avatar/f70d81e4d74abcb1a68f7c27edd99b1a?s=32&d=mm&r=g",
            "srcset": {
              "2x": "http://0.gravatar.com/avatar/f70d81e4d74abcb1a68f7c27edd99b1a?s=64&d=mm&r=g"
