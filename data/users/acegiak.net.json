                  "photo": [
                    {
                      "value": "https://acegiak.net/proxy/media/19313/31MMKrJS9YN5BUVdZivi54BqluF0FrrkSPrBBWOgyF4=/aHR0cHM6Ly9zb3MtY2gtZGstMi5leG8uaW8vbWljcm9ibG9ncHViL2xvZ28tbWljcm9ibG9ncHViLnBuZw==/50",
                      "alt": "microblogpub's avatar"
